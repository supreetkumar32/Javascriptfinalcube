<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<script>
    var wow;
  function openWith(){
    //   wow=window.open("https://www.google.com/");

      wow=window.open("https://www.google.com/","height=500px","width=400px");
  }

  function closeWith(){
      wow.close();
  }
</script>
<body>
    <button onclick="openWith()">Open google</button>
    <button onclick="closeWith()">close google</button>
</body>
</html>
